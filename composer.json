{
    "name": "jpswade/laravel-database-tools",
    "description": "Laravel Database Tools",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "James Wade",
            "email": "jpswade@gmail.com"
        }
    ],
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Jpswade\\LaravelDatabaseTools\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jpswade\\LaravelDatabaseTools\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^7.2|8.0.*",
        "illuminate/support": "*",
        "illuminate/console": "*",
        "illuminate/filesystem": "*",
        "laravel/framework": "~5.1|6.*|7.*|8.*",
        "ext-fileinfo": "*",
        "spatie/db-dumper": "^2.21"
    }
}
