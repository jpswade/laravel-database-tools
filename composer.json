{
    "name": "jpswade/laravel-database-tools",
    "description": "Laravel Database Tools",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "James Wade",
            "email": "jpswade@gmail.com"
        }
    ],
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Jpswade\\LaravelDatabaseTools\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jpswade\\LaravelDatabaseTools\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^7.4|8.*",
        "illuminate/support": "*",
        "illuminate/console": "*",
        "illuminate/filesystem": "*",
        "laravel/framework": "~5.1|6.*|7.*|8.*|9.*",
        "ext-fileinfo": "*",
        "spatie/db-dumper": "^2.21",
        "ext-zip": "*"
    },
    "suggest": {
        "league/flysystem-aws-s3-v3": "Needed for db:getFromBackup command to use the S3 Driver.",
        "spatie/db-dumper": "Needed for db:dump command to use the S3 Driver."
    }
}
